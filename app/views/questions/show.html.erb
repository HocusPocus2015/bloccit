<h1><%= @question.title %></h1>
<p><%= @question.body %></p>
<p><%= @question.resolved %></p>

<div class="col-md-4">
  <%= link_to "Edit", edited_question_path(@question), class: 'btn btn-success' %>
  <%= link_to "Delete", @question, method: :delete, class: 'btn btn-danger',
  data: {confirm: "You are about to delete the question. Click to confirm." } %>
</div>
